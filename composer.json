{
  "name": "xingbei437/x_phpsms",
  "description": "Probably the most intelligent, elegant sms send library in php, fork form toplan/phpsms",
  "license": "MIT",
  "keywords": ["sms", "php", "phpsms", "sms library"],
  "authors": [
    {
      "name": "xingbei437",
      "email": "xingbei437@163.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "xingbei437/x_task-balancer": "dev-master",
    "jeremeamia/superclosure": "~2.2",
    "alibabacloud/client": "^1.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.0"
  },
  "autoload": {
    "psr-4": {
      "Toplan\\PhpSms\\": [
        "src/phpsms/",
        "src/phpsms/agents/",
        "src/phpsms/interfaces/"
      ],
      "Toplan\\PhpSms\\Facades\\": [
        "src/phpsms/facades/"
      ]
    },
    "classmap": [
      "src/phpsms/lib/"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/mowenedu/phpsms.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/mowenedu/task-balancer.git"
    }
  ]
}
